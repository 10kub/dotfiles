vim9script
# hide menu
set guioptions=cM

# make gVim automatically maximize
set lines=999 columns=999

# set guifont=Dejavu\ Sans\ Mono\ 13.5
set guifont=Jetbrains\ Mono\ NL\ Light\ 13.5
# set guifont=Fira\ Mono\ 14
# set guifont=Ubuntu\ Mono\ 16

# disable blink cursor
set guicursor+=a:blinkon0

# colorscheme lightsimple
